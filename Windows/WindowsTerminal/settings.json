                "delta": -1.5
            },
            "keys": "ctrl+numpad_minus"
        },
        {
            "command": {
                "action": "closeTab"
            },
            "keys": "ctrl+f4"
        },
        {
            "command": "duplicateTab",
            "keys": "ctrl+shift+enter"
        },
        {
            "command": "unbound",
            "keys": "ctrl+shift+d"
        }
    ],
    "confirmCloseAllTabs": false,
    "copyFormatting": "none",
    "copyOnSelect": false,
    "initialCols": 90,
    "initialRows": 36,
    "newTabMenu": [
